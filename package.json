{
  "name": "cooking-h5",
  "version": "2.1.0",
  "description": "A vue h5 template with Vant UI",
  "author": "Sunnie <sunniejs@163.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "stage": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "taobao": "npm i --registry=https://registry.npm.taobao.org",
    "egg:start": "egg-scripts start --daemon --title=egg-server-egg",
    "egg:stop": "egg-scripts stop --title=egg-server-egg",
    "egg:dev": "egg-bin dev",
    "egg:debug": "egg-bin debug",
    "egg:test": "npm run lint -- --fix && npm run test-local",
    "egg:test-local": "egg-bin test",
    "egg:cov": "egg-bin cov",
    "egg:lint": "eslint .",
    "egg:ci": "npm run lint && npm run cov",
    "egg:autod": "autod"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "egg": "^2.31.0",
    "egg-scripts": "^2.15.1",
    "lib-flexible": "^0.3.2",
    "lodash": "^4.17.15",
    "regenerator-runtime": "^0.13.5",
    "vant": "^2.10.2",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "egg-bin": "^4.16.4",
    "egg-ci": "^1.19.0",
    "egg-mock": "^3.26.0",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^9.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "postcss-pxtorem": "^5.1.1",
    "sass": "^1.43.2",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
